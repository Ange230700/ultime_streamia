{
  "name": "ultime_streamia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "postinstall": "prisma generate && rm -rf public/themes && cp -R node_modules/primereact/resources/themes public/themes",
    "seed": "prisma db seed",
    "migrate:dev": "prisma migrate dev --name init",
    "migrate:prod": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:setup": "npm run migrate:dev && npm run seed",
    "db:deploy": "npm run migrate:prod && npm run seed"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "15.3.2",
    "node-fetch": "^3.3.2",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.5",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.7.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.17.46",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.8.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,mjs,cjs,jsx,mjsx,ts,tsx,mtsx}": "eslint --cache --fix"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
